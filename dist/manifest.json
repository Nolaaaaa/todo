{
  "name": "Hello Chrome", 
  "version": "1.0", 
  "description": "Say hello to chrome. I am your first extension.", 
  "manifest_version": 2,
  "browser_action": { 
    "default_icon": "icon.png"
  },
  "chrome_url_overrides": {
      "newtab": "index.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self';"
}
